{
  "name": "tacto",
  "version": "1.0.8",
  "description": "A convenient and pure Javascript sorting library using composition and zero external dependencies.",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "format": "biome check --write src",
    "lint": "biome check src",
    "publish": "npm publish",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brian-dlee/tacto.git"
  },
  "keywords": [
    "nodejs",
    "javascript",
    "sort"
  ],
  "author": "Brian Lee <briandl92391@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brian-dlee/tacto/issues"
  },
  "files": [
    "dist/**/*"
  ],
  "homepage": "https://github.com/brian-dlee/tacto#readme",
  "devDependencies": {
    "rimraf": "^4.4.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.0.0"
  }
}
